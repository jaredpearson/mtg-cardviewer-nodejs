<div style="display: table;">
{{#sets}}
    <div style="display: table-row">
        <div style="display: table-cell"><a href="/sets/{{code}}">{{name}}</a></div>
        <div style="display: table-cell">{{code}}</div>
        <div style="display: table-cell">
            {{#if smallSymbolImageUrl}}
                <img src="{{smallSymbolImageUrl}}" title="{{name}}" class="set-symbol-16 set-symbol-{{code}}" />
            {{/if}}
        </div>
        <div style="display: table-cell">{{releaseDate}}</div>
    </div>
{{/sets}}
</div>